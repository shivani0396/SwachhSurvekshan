<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>insertcomplaint</servlet-name>
        <servlet-class>controler.insertcomplaint</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updatesearchcomplaint</servlet-name>
        <servlet-class>controler.updatesearchcomplaint</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchcomplaint</servlet-name>
        <servlet-class>controler.searchcomplaint</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>updatecomplaint</servlet-name>
        <servlet-class>controler.updatecomplaint</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletecomplaint</servlet-name>
        <servlet-class>controler.deletecomplaint</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controler.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>index</servlet-name>
        <servlet-class>controler.index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>feedback</servlet-name>
        <servlet-class>controler.feedback</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insertcomplaint</servlet-name>
        <url-pattern>/insertcomplaint</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updatesearchcomplaint</servlet-name>
        <url-pattern>/updatesearchcomplaint</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchcomplaint</servlet-name>
        <url-pattern>/searchcomplaint</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updatecomplaint</servlet-name>
        <url-pattern>/updatecomplaint</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletecomplaint</servlet-name>
        <url-pattern>/deletecomplaint</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>feedback</servlet-name>
        <url-pattern>/feedback</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
